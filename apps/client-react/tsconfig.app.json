{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "auto-imports.d.ts", "src/**/*", "src/**/*.json"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "types": ["unplugin-icons/types/react"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
