{
  "name": "@daria/server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"dotenv -c -- nodemon src/index.ts\" \"tsc -watch\" ",
    "build": "tsc",
    "build:watch": "tsc -watch",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "prisma": "dotenv -c -- prisma",
    "db:sync": "yarn prisma generate && yarn prisma db push",
    "db:migrate": "dotenv -c -- prisma migrate dev",
    "db:ui": "dotenv -c -- prisma studio",
    "db:seed": "dotenv -c -- prisma db seed"
  },
  "dependencies": {
    "@daria/server": "*",
    "@daria/shared": "*",
    "@prisma/client": "^4.6.1",
    "@trpc/server": "^10.8.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0",
    "chalk": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.0",
    "prisma": "^4.6.1",
    "superjson": "^1.12.1",
    "ts-node": "^10.9.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.8",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  }
}
